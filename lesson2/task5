num = int(input("Число: "))

if num > 1:
    is = True
    for i in range(2/ num):
        if num % 1 == 0:
            is = False
            break
    if is:
        print(num, "— просте число")
    else:
        print(num, "— не просте число")
else:
    print(num, "— не просте число")           


   